{
  "name": "chat-api",
  "version": "0.0.1",
  "engines": {
    "node": "8.16.1"
  },
  "private": true,
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "grunt",
    "test": "NODE_ENV=test mocha --recursive test",
    "test:coverage": "nyc npm test",
    "test:unit": "mocha --recursive test/middleware test/models test/routes",
    "test:integration": "mocha --recursive test/integration",
    "prettier": "prettier --write './**/*.{ts,js,css,json}'",
    "build": "rm -rf ./dist/ && babel ./ -d dist --presets es2015,stage-0 --ignore ./node_modules && cp -R ./app/views ./dist/app/"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cookie-parser": "^1.3.3",
    "cookie-session": "^2.0.0-beta.1",
    "cors": "^2.8.5",
    "express": "^4.13.3",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.1",
    "file-stream-rotator": "^0.1.0",
    "glob": "^6.0.4",
    "lodash": "^4.17.5",
    "method-override": "^2.3.0",
    "moment": "^2.29.4",
    "morgan": "^1.6.1",
    "mysql": "^2.8.0",
    "sequelize": "^3.5.1",
    "serve-favicon": "^2.3.0",
    "socket.io": "^2.0.4",
    "sqlite3": "^3.1.13",
    "tedious": "^1.15.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "chai": "^3.5.0",
    "debug": "^2.2.0",
    "grunt": "^1.0.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-develop": "^0.4.0",
    "load-grunt-tasks": "^3.2.0",
    "mocha": "^3.0.2",
    "nyc": "^10.0.0",
    "prettier": "^1.10.2",
    "request": "^2.60.0",
    "supertest": "^2.0.0",
    "time-grunt": "^1.2.1"
  }
}
